<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
   <data noupdate="1">
   <!-- sequence for student result model -->
       <record id="ir_sequence_student_result" model="ir.sequence">
           <field name="name">Student Result</field>
           <field name="code">student.result</field>
           <field name="prefix">RES</field>
           <field name="padding">5</field>
           <field name="number_next">1</field>
           <field name="number_increment">1</field>
           <field name="company_id" eval="False"/>
       </record>

       <!-- sequence for student model -->
       <record id="ir_sequence_school_student" model="ir.sequence">
           <field name="name">Student id</field>
           <field name="code">school.student</field>
           <field name="prefix">STU</field>
           <field name="padding">5</field>
           <field name="number_next">1</field>
           <field name="number_increment">1</field>
           <field name="company_id" eval="False"/>
       </record>

       <!-- sequence for teacher model -->
       <record id="ir_sequence_school_teacher" model="ir.sequence">
           <field name="name">Teacher id</field>
           <field name="code">school.teacher</field>
           <field name="prefix">TEC</field>
           <field name="padding">5</field>
           <field name="number_next">1</field>
           <field name="number_increment">1</field>
           <field name="company_id" eval="False"/>
       </record>

       <!-- sequence for school model -->
       <record id="ir_sequence_school" model="ir.sequence">
           <field name="name">School id</field>
           <field name="code">school.profile</field>
           <field name="prefix">SCH</field>
           <field name="padding">5</field>
           <field name="number_next">1</field>
           <field name="number_increment">1</field>
           <field name="company_id" eval="False"/>
       </record>

       <!-- sequence for school model -->
       <record id="ir_sequence_school_subject" model="ir.sequence">
           <field name="name">Subject id</field>
           <field name="code">school.subject</field>
           <field name="prefix">SUB</field>
           <field name="padding">5</field>
           <field name="number_next">1</field>
           <field name="number_increment">1</field>
           <field name="company_id" eval="False"/>
       </record>
       <record id="ir_sequence_course" model="ir.sequence">
            <field name="name">Course id</field>
            <field name="code">provided.course</field>
            <field name="prefix">COR</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="ir_sequence_school_order" model="ir.sequence">
            <field name="name">school order</field>
            <field name="code">school.order</field>
            <field name="prefix">SORD/</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="company_id" eval="False"/>
        </record>
   </data>
</odoo>
